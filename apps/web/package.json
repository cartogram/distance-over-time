{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "build": "remix build",
    "deploy": "wrangler publish",
    "dev:remix": "remix watch -p 3005",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.7.2",
    "@remix-run/cloudflare-workers": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "cross-env": "^7.0.3",
    "react": "^18.0.2",
    "react-dom": "^18.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@remix-run/dev": "^1.7.2",
    "eslint-config-cartogram": "*",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.06",
    "eslint": "^8.23.1",
    "miniflare": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4",
    "wrangler": "^2.0.22"
  },
  "engines": {
    "node": ">=16.13"
  }
}
